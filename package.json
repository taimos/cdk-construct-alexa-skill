{
  "name": "@taimos/cdk-construct-alexa-skill",
  "version": "0.0.1",
  "description": "An AWS CDK Construct that creates an Alexa Skill backend",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf dist doc 'lib/**/*.js' 'lib/**/*.d.ts' 'tst/**/*.js' 'tst/**/*.d.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "doc": "typedoc --excludeExternals --mode file --out doc lib/",
    "build": "npm-run-all clean build:jsii test",
    "build:jsii": "jsii",
    "package": "npm-run-all build doc && jsii-pacmak"
  },
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "taimos.cdk-construct-alexa-skill",
        "module": "taimos.cdk_construct_alexa_skill"
      },
      "java": {
        "package": "de.taimos.cdk.construct.alexa.skill",
        "maven": {
          "groupId": "de.taimos.cdk.construct",
          "artifactId": "alexa-skill"
        }
      }
    }
  },
  "keywords": [
    "cdk",
    "aws-cdk",
    "construct",
    ""
  ],
  "author": {
    "email": "thorsten.hoeger@taimos.de",
    "name": "Thorsten Hoeger",
    "url": "https://www.taimos.de"
  },
  "awscdkio": {
    "twitter": "hoegertn"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/taimos/cdk-construct-alexa-skill"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.30.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.2",
    "concurrently": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "jest": "^25.1.0",
    "rimraf": "^3.0.2",
    "jsii": "^0.22.0",
    "jsii-pacmak": "^0.22.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.7.0",
    "typedoc": "^0.17.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@aws-cdk/core": "1.30.0",
    "constructs": "1.1.2",
    "@aws-cdk/aws-dynamodb": "1.30.0",
    "@aws-cdk/aws-iam": "1.30.0",
    "@aws-cdk/aws-lambda": "1.30.0",
    "@aws-cdk/aws-s3": "1.30.0"
  },
  "peerDependencies": {
    "constructs": "1.1.2",
    "@aws-cdk/core": "1.30.0",
    "@aws-cdk/aws-dynamodb": "1.30.0",
    "@aws-cdk/aws-iam": "1.30.0",
    "@aws-cdk/aws-lambda": "1.30.0",
    "@aws-cdk/aws-s3": "1.30.0"
  }
}
